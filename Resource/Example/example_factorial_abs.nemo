int abs( int number ){
    if( number < 0 ){
        number = -1 * number;
    }
    return number;
}

int fact( int n ){
    int result;
    result = 1;
    while( n > 0 ){
        result = result * n;
        n = n - 1;
    }
    return result;
}

void main(){
    int a;
    a = -7;
    
    a = fact( abs( a ) );
    
    print( a );
}
